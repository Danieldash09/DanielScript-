Section:NewButton("Mm2 Script Simple Gui Keyless Pc Mobile", "Simple Gui", function()-- Gui to Lua
-- Version: 3

-- Instances:

local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
